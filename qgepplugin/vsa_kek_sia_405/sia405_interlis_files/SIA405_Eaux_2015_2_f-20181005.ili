      ),
      polyethylene,
      mortier_de_ciment,
      emaillee,
      bitumee,
      zinc_epoxy
    );
    Mode_de_pose: (        
      inconnu,
      fouille_ouverte,
      sans_fouille,
      pousse_tube,
      pose_libre
    );
    Assurance_contre_la_poussee: (        
      inconnue,
      exterieure,
      interieure,
      aucune,
      joint_Duker,
      joint_Duro,
      anneau_de_verrouillage
    );
    Couverture: SIA405_Base_f.Couverture;  !! valeur moyenne d’un objet [m]
