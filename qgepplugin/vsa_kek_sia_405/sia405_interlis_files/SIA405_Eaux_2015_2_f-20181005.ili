      aucune,
      revetement_de_bitume,
      caoutchouc_chlore,
      mortier_de_fibrociment,
      galvanise_polyurethane,
      polyethylene_renforce,
      film_de_polyethylene,
      jute,
      enveloppe_bitumineuse,
      bande_matiere_synthetique,
      bande_bitumineuse,
      manchette_retractable
    );
