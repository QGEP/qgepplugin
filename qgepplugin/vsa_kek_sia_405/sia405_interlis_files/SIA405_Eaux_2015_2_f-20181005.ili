      aucune,
      courant_continu,
      courant_alternatif,
      anode_sacrificielle
    );
    Zone_de_pression: MANDATORY TEXT*30;
    Pression_de_fonctionnement_admissible: 0.0 .. 99.9 [Units.bar];
