CLASS TRONCON EXTENDS ELEMENT_RESEAU_EVACUATION =  
!! Elément de transport du réseau de canalisations hydrauliquement homogène, section utilisée pour une simulation de l'écoulement
  ATTRIBUTE
    PROTECTION_INTERIEURE: (          !! Protection de la paroi intérieur de la canalisation
      autre,
      peinture_revetement,
      revetement_en_brique,
      revetement_en_gres,
      inconnue,
      revetement_en_mortier_de_ciment
    );
    L_EFFECTIVE: 0.00 .. 30000.00 [m];  !! Longueur effective (non projetée) incluant les parties incurvées
    DETERMINATION_PLANIMETRIQUE: (          !! Définit la précision de la détermination du tracé.
      precise,  !! +/- 10 cm, lors de la détermination du tracé par différentes mesures, le triple, c'est-à-dire +/- 30 cm (norme SIA 405 1998, point 4 23 3)
      inconnue,
      imprecise  !! voir inconnue
    );
    HAUTEUR_MAX_PROFIL: Hauteur_max_profile;  !! Hauteur intérieure maximale du profil
    MATERIAU: (          !! Matériau du tuyau
      autre,
      amiante_ciment,
      beton_normal,
      beton_coule_sur_place,
      beton_pousse_tube,
      beton_special,
      beton_inconnu,
      fibrociment,
      terre_cuite,
      fonte_ductile,
      fonte_grise,
      matiere_synthetique_resine_d_epoxy,
      matiere_synthetique_polyethylene_dur,
      matiere_synthetique_polyester_GUP,
      matiere_synthetique_polyethylene,
      matiere_synthetique_polypropylene,
      matiere_synthetique_PVC,  !! polychlorure de vinyle
      matiere_synthetique_inconnue,  !! matière synthétique inconnu
      acier,
      acier_inoxydable,
      gres,
      argile,
      inconnu,
      ciment
    );
    PENTE_PLAN: Pente_promille;  !! Pente indiquée sur d'anciens plans non contrôlée [%o]. Ne peut pas être reprise pour des calculs hydrauliques. Indication pour des canalisations de biens-fonds ou d'amélioration foncière. Représentation sur de plan: 3.5‰ p
    K_STRICKLER: Strickler;  !! Constante de rugosité selon Manning-Strickler (K ou kstr);
    RELINING_GENRE: (          !! Genre du relining
      troncon_entier,
      partiellement,
      inconnu
    );
    RELINING_TECHNIQUE: (          !! Relining technique de construction. En addition la construction du reliner doit être modeler comme événement maintenance: Genre = reparation pour liner_partiel, autrement genre = renovation.
      autre,
      close_fit_relining,
      relining_tube_court,
      Noppenschlauchrelining,  !! terme française inconnue (english: Grouted in place lining)
      liner_partiel,
      chemisage_par_ligne_de_tuyau,  !! Chemisage par ligne de tuyau
      chemisage_par_gainage,  !! Hhemisage par gainage
      inconnu,
      chemisage_par_tube_spirale  !! Chemisage par tube spirale
    );
    RELINING_MATERIAUX: (          !! Materiaux du relining
      autre,
      resine_epoxy_lamine_en_fibre_de_verre,  !! résine epoxy lamine en fibre de verre
      resine_epoxy_feutre_synthetique,  !! résine epoxy feutre synthetique
      tuyau_PRV,  !! Polyester 	non saturé renforcé de fibres de verre (PRV),  tuyau HOBAS.  En allemand GF-UP
      HDPE,
      isocyanat_resine_lamine_en_fibre_de_verre,  !! isocyanat résine lamine en fibre de verre
      isocyanat_resine_lamine_feutre_synthetique,  !! isocyanat résine lamine feutre synthetique
      resine_de_polyester_lamine_en_fibre_de_verre,  !! résine de polyester lamine en fibre de verre
      resine_de_polyester_feutre_synthetique,  !! résine de polyester feutre synthetique
      polypropylene,
      PVC,
      radier_avec_pellicule_en_beton_polyester,  !! radier avec pellicule en béton polyester
      inconnu,
      UP_resine_LED_fibre_synthetiques_liner,  !! Fibre synthetiques liner 	avec résine polyester non saturé (UP résine) UP, durcissement avec UV-LED
      resine_d_ester_vinylique_lamine_en_fibre_de_verre,  !! Résine d'ester vinylique lamine en fibre de verre
      resine_d_ester_vinylique_feutre_synthetique  !! Résine d'ester vinylique feutre synthetique
    );
    RELINER_DIAMETRE_NOMINALE: Hauteur_max_profile;  !! Hauteur intérieure maximale du profil de l'inliner. A l'export dans le software hydraulique il faut utiliser cette attribut au lieu de HAUTEUR_MAX_PROFIL
    RIGIDITE_ANNULAIRE: 0 .. 16;  !! Rigidité annulaire pour des pressions extérieures (selon ISO 13966);
    TRACE: Base_f_LV95.Polyligne;  !! Points de départ, intermédiaires et d’arrivée de la conduite.
    K_COLEBROOK: Prandtl;  !! Coefficient de rugosité d'après Prandtl Colebrook (ks ou kb);
END TRONCON;

